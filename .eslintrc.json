{
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
  "rules": {
    "no-undef": ["warn"],
    "no-console": [ "error", { "allow": ["warn", "error"] } ]
  },
  "ignorePatterns": [
    "node_modules/",
    "gulpfile.js",
    "js/cl.built.js",
    "js/blocks.built.js",
    "js/wysiwyg/noneditable/**"
  ]
}
